/* pages/my-self/index.wxss */
/**index.wxss**/

.container{
  justify-content: flex-start;
  align-items: flex-start;
  flex-direction: row;
  margin-bottom: 30rpx;
}
.container-name{
  flex-direction: column;
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #aaa;
}

.userinfo-avatar {
  overflow: hidden;
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.usermotto {
  margin-top: 200px;
}

.userinfo-nickname{
  margin: 20rpx;
}
.phone-number{
  margin-left: 20rpx;
  background-color: white;
  border: green solid 2rpx;
  height: 45rpx;
  width: 140rpx;
  color: black;
  line-height: 40rpx;
  font-size: 20rpx;
}

.title{
  margin-left: 20rpx;
  color:gray;
  font-size: 25rpx;

}

.my-icon{
  /* margin-left: 20rpx; */
  background:#ffffff;
  width: 48rpx;
  height: 48rpx;
  background-size: 48rpx;
  background-position: center;
  background-size: contain;
  background-repeat: no-repeat;
 
}

.my-icon-star{
  background-image: url('data:image/png;base64,');
}

.my-icon-img{
  background-image: url('data:image/png;base64,');
  background-size: 36rpx;
}

.my-icon-service{
  background-image: url('data:image/png;base64,');
}

.my-icon-help{
  background-image: url('data:image/png;base64,');
}

.my-icon-feedback{
  background-image: url('data:image/png;base64,');
}


.cell-slot-container{
  display: flex;
  justify-content: flex-start;
  width: 70vw;
}

.cell-slot-container-normal{
  width: 100%;
}

.van-cell-text{
  width: 100%;
  margin-left: 10rpx;
  font-size: 24rpx;
  font-weight: 300;
}

.van-cell-text-btn{
  width: 100%;
  background-color: transparent;
  border:none;
  border-radius: 0;
  padding: 0;
  margin:0;
  margin-right: 0;
  box-sizing: content-box;
  padding-left: 0;
  padding-right: 0;
  text-align: left;
  outline: none;
  border-width: 0;
  outline: transparent;
}

.van-cell-text-btn::after{
  border:none;
}

.user-agreement{
  margin-top: 50rpx;
  margin-left: 10rpx;
  font-size: 24rpx;
  font-weight: 300;
  width: 100%;
  text-align: center;
}

.user-agreement-item{
  text-decoration: underline
}
