.talbe-container{
  box-sizing: border-box;
  margin: 0;
  padding: 5rpx;
  color: #000000d9;
  font-variant: tabular-nums;
  line-height: 1.5715;
  list-style: none;
  font-feature-settings: "tnum";
  position: relative;
  font-size: 14px;
  background: #fff;
  border-radius: 2px;
}
.table {
  /* border: 0px solid darkgray; */
  font-size: 20rpx;
  /* width: 100%; */
  text-align: left;
  border-radius: 2px 2px 0 0;
  border-collapse: separate;
  border-spacing: 0;
  border: 2px solid #f0f0f0;
 }
 .tr {
  display: flex;
  width: 100%;
  justify-content: center;
  /* height: 2rem; */
  /* border: gray solid 1rpx; */
  align-items: center;
 }
 .td {
  width:100%;
  height: 100%;
  /* height: 30rpx; */
  justify-content: center;
  text-align: flex-start;
  border-right: 1px solid #f0f0f0;
  border-bottom: 1px solid #f0f0f0;
 }

 .td:first{
  border-left:none;
 }

 .td:last-child{
  border-right:none;
 }
 .bg-w{
  /* background: snow; */
 }
 .bg-g{
  /* background: #E6F3F9; */
 }
 .th {
  width: 100%;
  justify-content: center;
  /* background: #3366FF; */
  color: black;
  font-weight: 700;
  display: flex;
  /* height: 3rem; */
  align-items: center;
 }

 .van-cell-text-btn{
  width: 100%;
  background-color: transparent;
  border:none;
  border-radius: 0;
  padding: 0;
  margin:0;
  margin-right: 0;
  box-sizing: content-box;
  padding-left: 0;
  padding-right: 0;
  text-align: left;
  outline: none;
  border-width: 0;
  outline: transparent;
}

.van-cell-text-btn::after{
  border:none;
}

.summary{
  font-size: 1.1rem;
  margin: 10rpx 0;
  color: green;
}